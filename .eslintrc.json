{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "consistent-return": "warn",
    "no-param-reassign": "off",
    "no-underscore-dangle": "warn",
    "prefer-promise-reject-errors": "warn"
  }
}
